{
    "name": "clankie-api",
    "version": "1.0.0",
    "description": "Clankie API - AI-powered business communication platform",
    "main": "src/app.js",
    "scripts": {
        "start": "node src/app.js",
        "dev": "nodemon src/app.js",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
        "seed": "npx sequelize-cli db:seed:all",
        "seed:undo": "npx sequelize-cli db:seed:undo:all",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:ai": "node test-ai-integration.js"
    },
    "keywords": [
        "api",
        "express",
        "sequelize",
        "ai",
        "business",
        "communication"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "sequelize": "^6.35.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "dotenv": "^16.3.1",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "compression": "^1.7.4",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "openai": "^4.20.1",
        "axios": "^1.6.2"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "sequelize-cli": "^6.6.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "@types/jest": "^29.5.8"
    }
}
